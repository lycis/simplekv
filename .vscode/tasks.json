{
    "tasks": [
      {
        "type": "cppbuild",
        "label": "build win64 server",
        "command": "C:\\msys64\\clang64\\bin\\clang.exe",
        "args": [
          "-fdiagnostics-color=always",
          "-g",
          "${workspaceFolder}\\server.c",
          "${workspaceFolder}\\kvstore.c",
          "-o",
          "${workspaceFolder}\\dist\\\\server.exe",
          "-lws2_32"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": [
          "$gcc"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "type": "cppbuild",
        "label": "build win64 client",
        "command": "C:\\msys64\\clang64\\bin\\clang.exe",
        "args": [
          "-fdiagnostics-color=always",
          "-g",
          "${workspaceFolder}\\client.c",
          "-o",
          "${workspaceFolder}\\dist\\\\client.exe",
          "-lws2_32"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": [
          "$gcc"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "type": "cppbuild",
        "label": "build cmunit framework example",
        "command": "C:\\msys64\\clang64\\bin\\clang.exe",
        "args": [
          "-fdiagnostics-color=always",
          "-g",
          "${workspaceFolder}\\cmunit_example.c",
          "-o",
          "${workspaceFolder}\\dist\\\\cmunit_example.exe"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": [
          "$gcc"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "type": "cppbuild",
        "label": "build: server unit test",
        "command": "C:\\msys64\\clang64\\bin\\clang.exe",
        "args": [
          "-D UNIT_TEST",
          "-lws2_32",
          "-fdiagnostics-color=always",
          "-static-libgcc",
          "-g",
          "${workspaceFolder}\\src\\server.c",
          "${workspaceFolder}\\src\\kvstore.c",
          "${workspaceFolder}\\src\\utilfuns.c",
          "${workspaceFolder}\\src\\server_unit_tests.c",
          "-o",
          "${workspaceFolder}\\dist\\\\server_test.exe"
        ],
        "options": {
          "cwd": "${fileDirname}"
        },
        "problemMatcher": [
          "$gcc"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "zig: build tests",
        "type": "shell",
        "command": "zig test --test-no-exec -femit-bin='zig-out/bin/server-zig-test.exe' -fno-strip -O Debug -DUNIT_TEST -lc -lws2_32 -idirafter.\\src\\ .\\test\\server_test.zig .\\src\\kvstore.c .\\src\\server.c .\\src\\utilfuns.c",
        "problemMatcher": [
          "$zig"
        ]
      }
    ],
    "version": "2.0.0"
  }
  